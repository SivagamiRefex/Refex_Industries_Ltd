<!-- start Simple Custom CSS and JS -->
<script type="text/javascript">
jQuery(document).ready(function ($) {
    // Function to perform the AJAX request
    function filterEvents(paged = 1) {
        var $form = $('.events-filter');
        var search = $form.find('[name="search"]').val(); // Get search value
        var sort = $form.find('[name="sort"]').val(); // Get current sort value
        var ajaxUrl = $form.data('ajax-url'); // Get AJAX URL

        // Perform the AJAX request
        $.ajax({
            url: ajaxUrl,
            type: 'POST',
            data: {
                action: 'events_filter', // Action that will match with the PHP function
                search: search,
                sort: sort,
                paged: paged, // Pass current page number for pagination
            },
            beforeSend: function () {
                $('#events-results').html('<p>Loading...</p>'); // Show loading message
            },
            success: function (response) {
                if (response) {
                    // Update the events results container
                    $('#events-results').html(response.html);

                    // Update the pagination count dynamically
                    $('#pagination-counts').text(response.pagination_count);

                    // Update pagination links
                    $('.events-pagination').html(response.pagination);
                }
            },
            error: function () {
                $('#events-results').html('<p>An error occurred. Please try again.</p>');
            },
        });
    }

    // Prevent Enter key from submitting the form
    $('.events-filter [name="search"]').on('keypress', function (e) {
        if (e.which === 13) { // Check if Enter key is pressed
            e.preventDefault(); // Prevent form submission
            filterEvents(); // Trigger the filter function
        }
    });

    // Trigger filter on sort dropdown change
    $('.events-filter [name="sort"]').on('change', function () {
        filterEvents(); // Call the filter function
    });

    // Trigger filter on search input keyup (real-time)
    $('.events-filter [name="search"]').on('keyup', function () {
        filterEvents(); // Call the filter function
    });

    // Handle pagination clicks
    $(document).on('click', '.events-pagination a', function (e) {
        e.preventDefault(); // Prevent default link behavior

        var $link = $(this);
        var paged = $link.attr('href').split('paged=')[1]; // Extract page number from the URL
        filterEvents(paged); // Call the filter function with the new page number
    });
});
</script>
<!-- end Simple Custom CSS and JS -->
<!-- start Simple Custom CSS and JS -->
<script type="text/javascript">
jQuery(document).ready(function ($) {
    // Function to perform the AJAX request
    function filterPressReleases(paged = 1) {
        var $form = $('.press-releases-filter');
        var search = $form.find('[name="search"]').val(); // Get search value
        var sort = $form.find('[name="sort"]').val(); // Get current sort value
        var ajaxUrl = $form.data('ajax-url'); // Get AJAX URL
        // Perform the AJAX request
        $.ajax({
            url: ajaxUrl,
            type: 'POST',
            data: {
                action: 'press_releases_filter',
                search: search,
                sort: sort,
                paged: paged, // Pass current page number
            },
            beforeSend: function () {
                $('#press-releases-results').html('<p>Loading...</p>');
            },
            success: function (response) {
                if (response) {
                    // Update the results container
                    $('#press-releases-results').html(response.html);

                    // Update pagination count dynamically
                    $('#pagination-count').text(response.pagination_count);

                    // Update pagination links
                    $('.press-releases-pagination').html(response.pagination);
                }
            },
            error: function () {
                $('#press-releases-results').html('<p>An error occurred. Please try again.</p>');
            },
        });
    }

    // Prevent Enter key from submitting the form
    $('.press-releases-filter [name="search"]').on('keypress', function (e) {
        if (e.which === 13) { // Check if Enter key is pressed
            e.preventDefault(); // Prevent form submission
            filterPressReleases(); // Trigger the filter function
        }
    });

    // Trigger filter on sort dropdown change
    $('.press-releases-filter [name="sort"]').on('change', function () {
        filterPressReleases(); // Call the filter function
    });

    // Trigger filter on search input keyup (real-time)
    $('.press-releases-filter [name="search"]').on('keyup', function () {
        filterPressReleases(); // Call the filter function
    });

    // Handle pagination clicks
    $(document).on('click', '.press-releases-pagination a', function (e) {
        e.preventDefault();

        var $link = $(this);
        var paged = $link.attr('href').split('paged=')[1]; // Extract page number
        filterPressReleases(paged); // Call the filter function with page number
    });
});
</script>
<!-- end Simple Custom CSS and JS -->
