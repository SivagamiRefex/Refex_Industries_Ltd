		<div class="default no-position show_shadow rs-sticky-default elementor-element elementor-element-8ad8c26 e-con-full e-flex e-con e-child" data-particle_enable="false" data-particle-mobile-disabled="false" data-id="8ad8c26" data-element_type="container">
		
				<div class="elementor-element elementor-element-7cf90db elementor-widget elementor-widget-shortcode" data-id="7cf90db" data-element_type="widget" data-widget_type="shortcode.default">
				<div class="elementor-widget-container">
							<div class="elementor-shortcode">    <div class="stock-quote-btn">
        <button id="stockQuoteNSE">NSE</button>
        <button id="stockQuoteBSE">BSE</button>
    </div>
    <div id="stock-quote" class="table-responsive">Loading stock data...</div>
    <div class="price-delay-note">
        <p id="stock-quote-time-note" >Pricing delayed by 5 minutes</p>
        <p id = "stock-quote-time"></p>
    </div>
    <script>

        jQuery(document).ready(function() {
            const route = 'https://refex.co.in/wp-json/' ;
            function fetchStockQuote(stockName) {
                jQuery.ajax({
                    
                    // url: '/wp-json/stock-api/v1/get-stock-quote-value', // REST API endpoint
                    url: `${route}stock-api/v1/get-stock-quote-value`,
                    method: 'POST',
                    contentType: 'application/json',
                    data: JSON.stringify({ stock_name: stockName }), // Replace 'REFEX.BO' with dynamic stock name if needed
                    success: function(response) {
                        if (response.status) {
                            let tableHTML = '<table class="table product-table" border="1" style="width:100%; border-collapse:collapse;">';
                            tableHTML += `
                                <tr>
                                    <th>CURRENCY</th>
                                    <th>PRICE</th>
                                    <th>BID</th>
                                    <th>OFFER</th>
                                    <th>CHANGE IN (%)</th>
                                    <th>VOLUME</th>
                                </tr>
                                <tr>
                                    <td>Rupees</td>
                                    <td>${response.data.current_price}</td>
                                    <td>${response.data.bid}</td>
                                    <td>${response.data.offer}</td>
                                    <td>${response.data.change}</td>
                                    <td>${response.data.volume}</td>
                                </tr>
                                <tr>
                                    <th>TODAY'S OPEN</th>
                                    <th>PREVIOUS CLOSE</th>
                                    <th>INTRADAY HIGH</th>
                                    <th>INTRADAY LOW</th>
                                    <th>52 WEEK HIGH</th>
                                    <th>52 WEEK LOW</th>
                                </tr>
                                <tr>
                                    <td>${response.data.open}</td>
                                    <td>${response.data.previousClose}</td>
                                    <td>${response.data.high}</td>
                                    <td>${response.data.low}</td>
                                    <td>${response.data.fiftyTwoWeekHigh}</td>
                                    <td>${response.data.fiftyTwoWeekLow}</td>
                                </tr>
                                
                                
                                `;
                            tableHTML += '</table>';
                            jQuery('#stock-quote').html(tableHTML);
                        } else {
                            jQuery('#stock-quote').html('<p>Error: ' + response.message + '</p>');
                        }
                    },
                    error: function() {
                        jQuery('#stock-quote').html('<p>Data could not be fetched. Please check your inputs.</p>');
                    }
                });
            }
            function getCurrentTimeFormatted() {
                const now = new Date();
                const day = String(now.getDate()).padStart(2, '0');
                const month = String(now.getMonth() + 1).padStart(2, '0'); // Months are 0-indexed
                const year = now.getFullYear();
                const hours = now.getHours();
                const minutes = String(now.getMinutes()).padStart(2, '0');
                const ampm = hours >= 12 ? 'PM' : 'AM';
                const formattedHours = hours % 12 || 12; // Convert to 12-hour format

                return `${day}/${month}/${year} ${formattedHours}:${minutes} ${ampm}`;
            }
            fetchStockQuote('REFEX.NS');
            jQuery('#stock-quote-time').text('Last Updated ' + getCurrentTimeFormatted());
            jQuery('#stockQuoteNSE').addClass('active');
            jQuery('#stockQuoteNSE').click(function () {
                jQuery('#stockQuoteBSE').removeClass('active');
                jQuery(this).addClass('active');
                jQuery('#stock-quote-time').text('Loading Stock Quote');
                fetchStockQuote('REFEX.NS');
                jQuery('#stock-quote-time').text('Last Updated ' + getCurrentTimeFormatted());
            });

            jQuery('#stockQuoteBSE').click(function () {
                jQuery('#stockQuoteNSE').removeClass('active');
                jQuery(this).addClass('active');
                jQuery('#stock-quote-time').text('Loading Stock Quote');
                fetchStockQuote('REFEX.BO');
                jQuery('#stock-quote-time').text('Last Updated ' + getCurrentTimeFormatted());

            });
        });
    </script>
    
    </div>
						</div>
				</div>
				</div>
					</div>
				</div>
		
