			<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1000 20" preserveAspectRatio="none">
	<path class="elementor-shape-fill" d="M0,0v3c0,0,393.8,0,483.4,0c9.2,0,16.6,7.4,16.6,16.6c0-9.1,7.4-16.6,16.6-16.6C606.2,3,1000,3,1000,3V0H0z"/>
</svg>		</div>
		
				<div class="elementor-element elementor-element-b7a983d elementor-widget__width-initial elementor-widget elementor-widget-shortcode" data-id="b7a983d" data-element_type="widget" id="home-ticker" data-widget_type="shortcode.default">
				<div class="elementor-widget-container">
							<div class="elementor-shortcode"><div class="top-header-stockdata">    <div id="bse-data">Loading BSE Data...</div>
    <div id="nse-data">Loading NSE Data...</div>
    <script>
        jQuery(document).ready(function() {
            const route = 'https://refex.co.in/wp-json/' ;
            const path = 'https://refex.co.in/wp-content/plugins/refex-stock/' ;
           
            function fetchStockIndexes() {
                jQuery.ajax({
                    type: 'POST', // Use POST method
                    url: `${route}stock-api/v1/get-nse-value`,
                    contentType: 'application/json',
                    success: function(response) {
                        if (response) {
                            if(response['nse_data'].index){
                                showIndexPrice('#nse-data', response['nse_data'], 'NSE');
                                showIndexPrice('#bse-data', response['bse_data'], 'BSE');
                            }else{
                                console.log(response['nse_data']);
                                console.log(response['bse_data']);
                            }
                        } else {
                            showError("No Data Found.");
                        }
                    },
                    error: function(xhr) {
                        if (xhr.responseJSON && xhr.responseJSON.message) {
                            showError(xhr.responseJSON.message);
                        } else {
                            showError("Error fetching NSE data.");
                        }
                    }
                });
            }
           
            fetchStockIndexes();
            setInterval(fetchStockIndexes, 30000);

            function showIndexPrice(id, response, exchange) {
                var isPositive = typeof response.index === 'string' 
                    ? response.index.charAt(0) === '+' 
                    : response.index.toString().charAt(0) === '+';
                var arrowImg = isPositive 
                    ? `<img src="${path}img/up.png" alt="Up" style="width:30px; vertical-align:middle; margin-right:4px;">` 
                    : `<img src="${path}img/down.png" alt="Down" style="width:30px; vertical-align:middle; margin-right:4px;">`;
                var color = isPositive ? 'green' : 'red';
                // var formatted_overall_index = exchange === 'NSE' 
                //     ? parseFloat(response.index || 0).toFixed(2) 
                //     : response.overall_index;
                var content = `${exchange} ${arrowImg}<span style="color:${color};"> ${response.price} (${response.overall_index}%)</span>`;
                jQuery(id).html(content);
            }

            function showError(message) {
                console.log(message);
            }
        });
    </script>
    </div></div>
						</div>
				</div>
				</div>
		
