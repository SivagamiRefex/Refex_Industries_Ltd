<script type="text/javascript">
/* Default comment here */ 


const locations = {
    ash-coal-MP: {
        title: "Uttar Pradesh",
        details: [
            "Known for its rich history and culture.",
            "Home to the Taj Mahal.",
            "Famous cities include Lucknow, Varanasi, and Agra."
        ]
    },
    delhi: {
        title: "Delhi",
        details: [
            "Capital city of India.",
            "Known for historic monuments like India Gate and Red Fort.",
            "Famous for its vibrant street food and markets."
        ]
    },
    assam: {
        title: "Assam",
        details: [
            "Known for its tea plantations.",
            "Rich wildlife with Kaziranga National Park.",
            "The mighty Brahmaputra River flows through Assam."
        ]
    }
};

const map = document.getElementById("map");
const infoBox = document.getElementById("info-box");
const locationTitle = document.getElementById("location-title");
const locationDetails = document.getElementById("location-details");


function updateInfoBox(location) {
    locationTitle.textContent = location.title;
    locationDetails.innerHTML = location.details
        .map(detail => `<li>${detail}</li>`)
        .join("");
}


updateInfoBox(locations.up);
infoBox.style.display = "block"; 


map.addEventListener("mouseover", (event) => {
    const id = event.target.id;
    if (locations[id]) {
       
        updateInfoBox(locations[id]);
        infoBox.style.display = "block";  
    }
});

map.addEventListener("mouseout", () => {
   
    infoBox.style.display = "none";
    updateInfoBox(locations.ash-coal-MP);
    infoBox.style.display = "block";
});

</script>
<!-- end Simple Custom CSS and JS -->
<!-- start Simple Custom CSS and JS -->
<script type="text/javascript">
document.addEventListener('DOMContentLoaded', function () {
    const toggleBtns = document.querySelectorAll('.toggle-btn'); // Select all toggle buttons

    toggleBtns.forEach(btn => {
        btn.addEventListener('click', function () {
            const container = document.querySelector('.hidden-container'); // Select the container to toggle

            if (container) {
                if (container.style.display === 'none' || container.style.display === '') {
                    container.style.display = 'block'; // Show the container
                    btn.style.display = 'none'; // Hide the button that was clicked
                }
            }
        });
    });
});
</script>
<!-- end Simple Custom CSS and JS -->
